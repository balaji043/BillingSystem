<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CustomUI.EditBill.EditBill">
    <center>
        <JFXListView fx:id="listView" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <styleClass>
                <String fx:value="decrement-arrow" />
                <String fx:value="decrement-button" />
                <String fx:value="increment-arrow" />
                <String fx:value="increment-button" />
                <String fx:value="pretty-list-view" />
                <String fx:value="pretty-scroll-bar" />
                <String fx:value="scroll-bar" />
                <String fx:value="thumb" />
                <String fx:value="virtual-flow" />
            </styleClass>
        </JFXListView>
    </center>
   <bottom>
       <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
           <JFXButton text="Save"/>
           <JFXButton text="Cancel"/>
       </HBox>
   </bottom>
</BorderPane>
